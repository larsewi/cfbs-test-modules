bundle agent create_multiple_files_with_content
# @brief create multiple files with content.
{
  meta:
      "tags"
        slist => { "autorun" };

  vars:
      "indices"
        slist => getindices("cfbs:create_multiple_files_with_content.files");

  files:
      "$(cfbs:create_multiple_files_with_content.files[$(indices)][name])"
        content => "$(cfbs:create_multiple_files_with_content.files[$(indices)][content])";
}
